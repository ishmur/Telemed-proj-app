{
  "name": "TelemedDesktop",
  "version": "0.1.0",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron ./app/main.js",
    "dist": "npm run dist:win32 && npm run dist:win64",
    "dist:win32": "build --ia32",
    "dist:win64": "build --x64",
    "postinstall": "install-app-deps"
  },
  "devDependencies": {
    "electron-builder": "^4.2.6",
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^0.37.4",
    "electron-squirrel-startup": "^1.0.0"
  },
  "build": {
    "app-bundle-id": "Telemed.project.desktop",
    "app-category-type": "public.app-category.medical",
    "iconUrl": "https://d30y9cdsu7xlg0.cloudfront.net/png/50115-200.png",
    "win": {
      "setupIcon": "./build/icon.ico"
    }
  }
}
